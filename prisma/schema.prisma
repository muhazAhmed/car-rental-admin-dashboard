generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Car {
  id          String   @id @default(uuid())
  make        String
  model       String
  year        Int
  pricePerDay Int
  isAvailable Boolean
  imageUrl    String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
